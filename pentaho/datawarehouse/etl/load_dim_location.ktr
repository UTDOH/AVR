    <sql>SELECT &#xa;   a.id&#xa; , c1.id as county_id&#xa; , c1.code_description AS county&#xa; , c2.id as state_id&#xa; , c2.code_description AS state&#xa; , a.street_number&#xa; , a.street_name&#xa; , a.unit_number&#xa; , a.postal_code&#xa; , a.city&#xa; , a.longitude&#xa; , a.latitude&#xa; , a.entity_id&#xa; , a.entity_location_type_id&#xa; , a.event_id&#xa; , a.created_at&#xa; , a.updated_at&#xa;FROM addresses a&#xa;   LEFT JOIN external_codes c1 ON a.county_id &#x3d; c1.id&#xa;   LEFT JOIN external_codes c2 ON a.state_id &#x3d; c2.id&#xa;where a.updated_at &#x3e; &#x3f;&#x3b;</sql>
