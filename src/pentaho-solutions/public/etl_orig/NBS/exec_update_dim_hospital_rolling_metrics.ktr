  <step>
    <name>Get Rolling 12 mo Births by Hospital</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>NBSOLAPDS</connection>
    <rowlimit>0</rowlimit>
    <sql>select
	 fnc.dw_nbs_hospital_id
	,count(1) as rolling_hospital_num_births
    ,rolling_12_month_births.ct as rolling_state_num_births
	,cast(count(1) / rolling_12_month_births.ct::float as decimal(9,8)) as rolling_pct_of_births	
from nbs_olap.fact_nbs_card fnc,	
(
select 
	count(distinct kit_number) as ct
from nbs_olap.fact_nbs_card where dw_birth_date_num_id >= to_number(to_char(now(), 'YYYYMMDD'), '99999999') - 10000
) rolling_12_month_births
where fnc.dw_birth_date_num_id >= to_number(to_char(now(), 'YYYYMMDD'), '99999999') - 10000
group by dw_nbs_hospital_id, rolling_12_month_births.ct</sql>
