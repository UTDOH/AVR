    <sql>drop table if exists warehouse.dim_date_new&#x3b;&#xa;&#xa;CREATE TABLE warehouse.dim_date_new&#xa;&#x28;&#xa;  dw_date_id integer NOT NULL, -- Technical key of the date dimension&#xa;  dw_date_num_id integer NOT NULL, -- Alternate TK of the date dimension in the format YYYYMMDD&#xa;  hashcode integer, -- Type I dimension hashcode&#xa;  src_date_val date, -- Source date value&#xa;  date_short character&#x28;12&#x29;, -- Short date&#xa;  date_medium character&#x28;16&#x29;, -- Medium date&#xa;  date_long character varying&#x28;24&#x29;, -- Long form date&#xa;  date_full character varying&#x28;32&#x29;, -- Full form date&#xa;  day_in_week integer, -- day of the week&#xa;  day_in_month integer, -- Day number within the month&#xa;  day_in_year integer, -- Day number within the year&#xa;  is_first_day_in_month smallint, -- Flag indicating whether the date is the first day in the month&#xa;  is_first_day_in_week smallint, -- Flag indicating whether the date is the first day in the week.&#xa;  is_last_day_in_month smallint, -- Flag indicating whether the date is the last day in the month.&#xa;  is_last_day_in_week smallint, -- Flag indicating whether the date is the last day of the week&#xa;  day_name character varying&#x28;12&#x29;, -- textual name of the day of week&#xa;  day_abbreviation character&#x28;3&#x29;, -- Abbreviated day of week&#xa;  week_in_year integer, -- Numeric week of the year&#xa;  week_in_month integer, -- Week number within the month.&#xa;  week_in_year_iso integer, -- ISO standard week in year&#xa;  week_start_date date, -- The date representing the first day of the week.&#xa;  is_weekend smallint, -- Flag indicating wheter or not the day Is a Saturday or Sunday&#x3f;&#xa;  is_weekday smallint, -- Flag indicating whether or not the date is a weekday&#xa;  month_number integer, -- Month number &#x28;1..12&#x29;&#xa;  month_name_val character varying&#x28;12&#x29;, -- Name of the month&#xa;  month_abbreviation character&#x28;3&#x29;, -- Abbreviated month name&#xa;  month_abbreviation_year2 character&#x28;6&#x29;, -- Month abbreviation with digit year&#xa;  year2 integer, -- Two digit year&#xa;  year4 integer, -- Four digit year&#xa;  year2_iso integer, -- ISO two digit year&#xa;  year4_iso integer, -- ISO four digit year&#xa;  quarter_number integer, -- Numeric representation of the quarter &#x28;1..4&#x29;&#xa;  quarter_name character&#x28;2&#x29;, -- Name of the quarter &#x28;Q1..Q4&#x29;&#xa;  quarter_year character&#x28;7&#x29;, -- Quarter name &#x2b; year&#xa;  month_year character&#x28;7&#x29;, -- Month number - year4&#xa;  week_year character&#x28;7&#x29;, -- Week number - year4&#xa;  week_year_iso character&#x28;7&#x29;, -- week number - year4 ISO standard&#xa;  year_week_int integer, -- Year4 and week number as numeric value for sorting and comparison&#xa;  current_week_cy smallint, -- Is this the current week of the current year&#xa;  current_month_cy smallint, -- Is this the current month of the current year&#xa;  last_week_cy smallint, -- Is this the previous week of the current year&#xa;  last_month_cy smallint, -- Flag indicating whether this date in the previous month of the current year&#xa;  current_week_ly smallint, -- Is this date in the previous year, but the same week as the current week&#x3f;&#xa;  current_month_ly smallint, -- Is this date in the previous year, but the same month of the current year&#x3f;&#xa;  last_week_ly smallint, -- Is this date in the previous week the year prior&#x3f;&#xa;  last_month_ly smallint, -- Is this date in the previous month the year prior&#xa;  ytd_cy_day smallint, -- Is the day portion of the date in the current year&#x3f;&#xa;  ytd_cy_week smallint, -- Is the week portion of the date in the current year&#x3f;&#xa;  ytd_cy_month smallint, -- Is the month of the date in the current year&#x3f;&#xa;  ytd_ly_day smallint, -- Is this day part of the year to date last year &#x28;for current year &#x2f; previous YTD comparison calculations&#x29;&#xa;  ytd_ly_week smallint, -- Is the date in the week last year which spilled into this year&#x3f;&#xa;  ytd_ly_month smallint, -- Is this month part of the year to date last year &#x28;for current year &#x2f; previous YTD comparison calculations&#x29;&#xa;  current_year smallint, -- Is the date in the current year&#x3f;&#xa;  last_year smallint, -- Is the date in the previous year&#x3f;&#xa;  day_sequence integer, -- How many days back is this day from the current day&#x3f;&#xa;  week_sequence integer, -- How many weeks back from the current week is the week this date is in&#x3f;&#xa;  month_sequence integer, -- How many monts back from the current month is the month this date is in&#x3f;&#xa;  year_of_weekend_date integer, -- What year does the last day of the week fall in&#x3f;&#xa;  etl_load_date timestamp without time zone, -- The date this dimension record was loaded into the data warehouse.&#xa;  mmwr_week integer NOT NULL DEFAULT 0,&#xa;  mmwr_year integer NOT NULL DEFAULT 0,&#xa;  CONSTRAINT pk_dw_dim_date_new PRIMARY KEY &#x28;dw_date_id&#x29;&#xa;&#x29;&#xa;WITH &#x28;&#xa;  OIDS&#x3d;FALSE&#xa;&#x29;&#x3b;&#xa;&#xa;COMMENT ON TABLE warehouse.dim_date_new&#xa;  IS &#x27;Date dimension&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.dw_date_id IS &#x27;Technical key of the date dimension&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.dw_date_num_id IS &#x27;Alternate TK of the date dimension in the format YYYYMMDD&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.hashcode IS &#x27;Type I dimension hashcode&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.src_date_val IS &#x27;Source date value&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.date_short IS &#x27;Short date&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.date_medium IS &#x27;Medium date&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.date_long IS &#x27;Long form date&#x27;&#x3b;&#xa;COMMENT ON COLUMN warehouse.dim_date_new.date_full IS &#x