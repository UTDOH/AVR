  <step>
    <name>get processed_diagnosis</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>DCPLDW</connection>
    <rowlimit>0</rowlimit>
    <sql>select
	spd.ss_processed_message_id
    ,spd.diagnosis_code
    ,spd.diagnosis_description
    ,spd.diagnosis_code_system
	,case 
      when diagnosis_code is null then null 
	  when snomed_diagcd.id is null then 1 else 0 
     end as tf1_diagnosis_code_v
    ,case 
      when diagnosis_description is null then null 
	  when snomed_diagdesc.id is null then 1 else 0 
     end as tf1_diagnosis_description_v
from syndromic.ss_processed_diagnosis spd
left join syndromic.rt_icd10cm_icd9cm_snomedct snomed_diagcd on lower(diagnosis_code) like '%'||lower(snomed_diagcd.preferred_alternate_code)||'%' 
or lower(diagnosis_code) like '%'||lower(snomed_diagcd.concept_code)||'%'
left join syndromic.rt_icd10cm_icd9cm_snomedct snomed_diagdesc on lower(diagnosis_description) like '%'||lower(snomed_diagdesc.preferred_alternate_code)||'%' 
or lower(diagnosis_description) like '%'||lower(snomed_diagdesc.concept_code)||'%'
where spd.ss_processed_message_id >= ${min_processed_message_id} and spd.ss_processed_message_id &lt; ${max_processed_message_id}
group by 1,2,3,4,5,6
order by spd.ss_processed_message_id
-- limit 2000001</sql>
