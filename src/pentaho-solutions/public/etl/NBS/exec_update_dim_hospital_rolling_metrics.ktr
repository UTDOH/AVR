    <connection>Trisano OLAP DS</connection>
    <rowlimit>0</rowlimit>
    <sql>select&#xa;	 fnc.dw_hospital_id&#xa;	,count&#x28;1&#x29; as num_hospital_births&#xa;    ,rolling_12_month_births.ct as rolling_num_births&#xa;	,cast&#x28;count&#x28;1&#x29; &#x2f; rolling_12_month_births.ct&#x3a;&#x3a;float as decimal&#x28;9,8&#x29;&#x29; as rolling_pct_of_births	&#xa;from warehouse.fact_nbs_card fnc,	&#xa;&#x28;&#xa;select &#xa;	count&#x28;distinct kit_number&#x29; as ct&#xa;from warehouse.fact_nbs_card where dw_birth_date_num_id &#x3e;&#x3d; to_number&#x28;to_char&#x28;now&#x28;&#x29;, &#x27;YYYYMMDD&#x27;&#x29;, &#x27;99999999&#x27;&#x29; - 10000&#xa;&#x29; rolling_12_month_births&#xa;where fnc.dw_birth_date_num_id &#x3e;&#x3d; to_number&#x28;to_char&#x28;now&#x28;&#x29;, &#x27;YYYYMMDD&#x27;&#x29;, &#x27;99999999&#x27;&#x29; - 10000&#xa;group by dw_hospital_id, rolling_12_month_births.ct</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
